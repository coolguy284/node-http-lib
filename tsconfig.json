{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "noEmit": true
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "include": [
    "**/*.mjs"
  ]
}
